{
  "name": "@cher-ami/router",
  "version": "0.1.0",
  "description": "A fresh react router built for route transitions flexibility",
  "author": "Willy Brauner",
  "license": "MIT",
  "main": "src/index.ts",
  "private": false,
  "keywords": [
    "router",
    "react",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/cher-ami/router.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "parcel example/index.html --out-dir example/dist --open",
    "build": "parcel build example/index.html --out-dir example/dist",
    "compile": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "history": "^5.0.0",
    "path-parser": "^6.1.0"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.19",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "gsap": "^3.3.4",
    "parcel-bundler": "^1.12.4",
    "preact": "^10.5.9",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{tsx,ts,jsx,js,less,css,json,md}": [
      "prettier --write"
    ]
  }
}
